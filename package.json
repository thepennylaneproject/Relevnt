{
  "name": "relevnt-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Relevnt - AI-powered career intelligence platform",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit || true && vite build",
    "preview": "vite preview",
    "dev:netlify": "netlify dev",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:css": "stylelint \"src/**/*.css\" --allow-empty-input",
    "lint:all": "npm run lint && npm run lint:css",
    "test": "vitest",
    "test:jest": "jest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "clsx": "^2.0.0",
    "fast-xml-parser": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.555.0",
    "mammoth": "^1.11.0",
    "openai": "^6.8.1",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^5.4.394",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@netlify/functions": "^5.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.24",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "stylelint": "^16.12.0",
    "stylelint-config-standard": "^36.0.1",
    "dotenv": "^17.2.3",
    "jsdom": "^27.2.0",
    "netlify-cli": "^23.12.3",
    "supabase": "^2.58.5",
    "tsx": "^4.21.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
